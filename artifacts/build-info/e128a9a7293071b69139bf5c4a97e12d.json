			"FundMe/Owner.sol": {
				"content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.18;\n\ncontract Owner {\n    address immutable i_owner;\n\n    constructor() {\n        i_owner = msg.sender;\n    }\n\n    modifier onlyOwner() {\n        require(msg.sender == i_owner, \"Not the owner\");\n        _;\n    }\n}\n"
			}
